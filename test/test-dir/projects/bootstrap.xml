<project>
  <name id="bootstrap" />
  <scm id="git" />

  <variable id="url" value="../../../../" />

  <action id="make">
    <cmd>make build</cmd>
  </action>

  <action id="regtests">
    <cmd>make regtests</cmd>
  </action>

  <scenario id="default">
    <scm_action id="version" status="require_change" on_error="quit" />
    <scm_action id="pull" />
    <action id="make" />
    <action id="regtests" />
  </scenario>
</project>
