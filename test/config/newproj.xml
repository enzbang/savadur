<project>
  <name id="newproj" />
  <scm id="git" />

  <variable id="url"
     value="../../../newproj" />

  <action id="make">
    <cmd>make</cmd>
  </action>

  <action id="regtests">
    <cmd>make regtests</cmd>
  </action>

  <scenario id="default">
    <scm_action id="version" require_change="true" on_error="quit" />
    <scm_action id="pull" />
    <action id="make" />
    <action id="regtests" />
  </scenario>
</project>
